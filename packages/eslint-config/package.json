{
  "name": "@antlered-viking/eslint-config",
  "version": "1.1.0",
  "license": "ISC",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore",
    "lint:fix": "eslint --ignore-path .gitignore --fix",
    "format": "prettier --ignore-path --plugin-search-dir=. .",
    "format:fix": "prettier --ignore-path --write --plugin-search-dir=. .",
    "install": "node ./lib/post-install.js"
  },
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0"
  },
  "dependencies": {
    "@antlered-viking/eslint-config": "^1.1.0",
    "@antlered-viking/prettier-config": "^1.1.1",
    "npm-add-script": "^1.1.0"
  },
  "prettier": "@antlered-viking/prettier-config"
}
